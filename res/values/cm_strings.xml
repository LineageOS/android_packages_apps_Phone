<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">

    <!-- Advanced Settings -->
    <string name="advanced_setting">Advanced Settings</string>
    <string name="vibrate_outgoing">Vibrate on answer</string>
    <string name="vibrate_call_waiting">Vibrate on call waiting</string>
    <string name="vibrate_hangup">Vibrate on hang up</string>
    <string name="vibrate_45">Vibrate every 45 seconds</string>
    <string name="vibrate_45_sum">Vibrates at the 45 second mark of every minute during outgoing calls</string>
    <string name="rejected_as_missed">Rejected calls as missed</string>
    <string name="rejected_as_missed_sum">Mark rejected calls as \'missed\' rather than \'incoming\' in the call log</string>
    <string name="show_ssn_label">Event notification</string>
    <string name="show_ssn_summary">Show a notification for various network events related to call handling</string>
    <string name="calllog_after_call">Show log</string>
    <string name="calllog_after_call_summary">Show the call log after ending a call</string>

    <!-- Mobile network settings screen, setting check box name -->
    <string name="national_roaming">National data roaming</string>
    <!-- Mobile network settings screen, setting summary text when check box is selected -->
    <string name="national_roaming_enable">Connect to data services when nationally roaming</string>
    <!-- Mobile network settings screen, setting summary text when check box is clear -->
    <string name="national_roaming_disable">Connect to data services when nationally roaming</string>

    <!-- Permission for notification-bar network toggle -->
    <string name="perm_change_mobile_network">Change network (2G/3G)</string>
    <string name="perm_change_mobile_network_desc">Application with this permission can, at any time, change between 2G data and 3G data. 3G is faster, consumes more battery, and can also cost you more money.</string>

    <!-- Setting title for the flip action setting. -->
    <string name="flip_action_title">Flip action</string>

    <!-- Dialog title of the flip action setting. -->
    <string name="flip_action_dialog_title">Flip device to\u2026</string>

    <!-- Setting summary for the flip action setting. -->
    <string name="flip_action_summary">Flipping the phone down will <xliff:g id="action">%s</xliff:g></string>

    <!-- Values for the flip action settings. -->
    <string-array name="flip_action_setting_values" translatable="false">
        <item>0</item>
        <item>1</item>
        <item>2</item>
    </string-array>

    <!-- Entries listed in the setting for the flip action. -->
    <string-array name="flip_action_setting_entries">
        <item>Do nothing</item>
        <item>Mute ringer</item>
        <item>Dismiss call</item>
    </string-array>

    <!-- Summary texts for flip action. Must be ordered by numerical
         value (do nothing = 0, mute = 1, dismiss = 2) -->
    <string-array name="flip_action_summary_entries">
        <item>do nothing</item>
        <item>mute the ringer</item>
        <item>dismiss the call</item>
    </string-array>

    <!-- Blacklist preferences -->
    <string name="blacklist_title">Blacklist</string>
    <string name="enable_blacklist_title">Enable blacklist</string>
    <string name="blacklist_summary_disabled">Disabled</string>
    <string name="blacklist_summary">Incoming calls from phone numbers in the blacklist will be blocked</string>
    <string name="blacklist_notify">Show notification</string>
    <string name="blacklist_regex_title">Use wildcards</string>
    <string name="blacklist_regex_summary">Use . as a wildcard and * for repetition. E.g. 123.* blocks numbers starting with 123 and .*123.* blocks numbers containing 123</string>

    <!-- Blacklist management -->
    <string name="add_blacklist_number">Add phone number</string>
    <string name="add_to_blacklist">Add to blacklist</string>
    <string name="add_to_blacklist_message">Future calls from %s will be blocked</string>
    <string name="remove_blacklist_number_title">Remove number</string>
    <string name="remove_blacklist_number">Do you want to remove %s from the blacklist?</string>

    <!-- Blacklist notifications -->
    <string name="blacklist_notification">Blocked a call from %s</string>
    <string name="unblock_number">Unblock</string>

    <!-- Text for the onscreen "Blacklist" button -->
    <string name="onscreenBlacklistText">Blacklist</string>

</resources>
